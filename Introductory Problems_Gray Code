//Gray Code
#include <stdio.h>
#include <math.h>
int main() {
    int n;
    scanf("%d", &n);
    int x = pow(2,n);
    for(int i = 0; i < x; i++){
        char arr[16] = {0};
        int j = 0;
        int k = i;
        while(k>0){
            arr[j] = k%2;
            k = k/2;
            j++;
        }
        for(int k = n-1; k >= 0; k--)
            printf("%d", arr[k]);
        printf("\n");
            
    }
}
